{
  "name": "buildfunctions",
  "version": "0.0.8",
  "description": "The Buildfunctions SDK for Agents: Hardware-isolated CPU and GPU Sandboxes for untrusted AI actions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildfunctions/sdk.git"
  },
  "keywords": [
    "AIAgents",
    "AIInfrastructure",
    "AIApps",
    "Serverless",
    "GPUs",
    "Sandboxes",
    "DeveloperExperience"
  ],
  "author": "Buildfunctions",
  "bugs": {
    "url": "https://github.com/buildfunctions/sdk/issues"
  },
  "homepage": "https://www.buildfunctions.com",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "25.0.10",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "dotenv": "17.2.3"
  }
}
